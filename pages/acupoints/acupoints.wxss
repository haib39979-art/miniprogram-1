.container {
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  position: relative;
}

.custom-nav {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  padding-top: calc(env(safe-area-inset-top) + 120rpx);
  height: 88rpx;
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  z-index: 100;
}

.close-btn {
  font-size: 90rpx;
  font-weight: 300;
  line-height: 1;
  color: #000;
  padding: 10rpx;
}

.content-scroll {
  width: 100%;
  height: 100%;
  padding-top: calc(env(safe-area-inset-top) + 140rpx);
  box-sizing: border-box;
}

.acupoint-image {
  width: 100%;
  display: block;
  margin-bottom: 20rpx;
}

.acupoint-image:last-child {
  margin-bottom: 0;
}
